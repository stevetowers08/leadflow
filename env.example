GITHUB_PERSONAL_ACCESS_TOKEN=your-github-personal-access-token

# MCP Server Configuration
PORT=3000
NODE_ENV=production

# AI Chat Module Configuration
N8N_WEBHOOK_URL=https://your-n8n-instance.com/webhook/your-webhook-id

